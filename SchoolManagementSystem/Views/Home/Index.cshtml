@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}

@using (Html.BeginForm("Login", "Home"))
{
<table>
    <tr>
        <td>
            UserName :
        </td>
        <td>
            @Html.TextBox("userName")
        </td>
    </tr>
    <tr>
        <td>
            Password : 
        </td>
        <td>
            @Html.TextBox("password")
        </td>
    </tr>
    <tr>
        <td colspan="2">
            <input type="submit" value="Login" />
        </td>
    </tr>
</table>
 

}